<%= form_for @sneaker do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :image_url %>
  <%= f.text_field :image_url %>

  <%= f.label :price %>
  <%= f.text_field :price %>

  <%= f.label :footlocker_url %>
  <%= f.text_field :footlocker_url %>

  <%= f.submit %>
<% end %>
